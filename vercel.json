{
  "version": 2,
  "builds": [
    {
      "src": "src/app-alternative.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "/src/app-alternative.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/app-alternative.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/app-alternative.js"
    }
  ],
  "functions": {
    "src/app-alternative.js": {
      "maxDuration": 30
    }
  }
}
